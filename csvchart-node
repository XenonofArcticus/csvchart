#!/usr/bin/env node
// vimrun! node -i -r ./csvchart-node

const fs = require("fs");
const CSVChart = require("./lib/csvchart.js");

// const argv = process.argv.slice(2);
// var d = fs.readFileSync("test/testfile-01.csv", "utf8");

fs.readFile("testfiles/testfile-01.csv", "utf8", (err, data) => {
	if(err) {
		console.error(err);

		return;
	}

	var d = CSVChart.ChartJS.parse(data);

	console.log(d);

	global.data = d;
});
